{
  "name": "heapify",
  "version": "0.2.1",
  "description": "Blazing fast JavaScript priority queue",
  "main": "heapify.mjs",
  "scripts": {
    "test": "nyc  --reporter=html --reporter=text mocha --require esm",
    "count": "cloc heapify.mjs",
    "bench": "node bench.mjs",
    "report-coverage": "nyc report --reporter=text-lcov --require esm > coverage.lcov && codecov"
  },
  "keywords": [
    "priority queue",
    "queue",
    "heap",
    "minheap",
    "maxheap",
    "binary heap"
  ],
  "author": {
    "name": "Lucio Paiva",
    "url": "https://luciopaiva.com"
  },
  "homepage": "https://github.com/luciopaiva/heapify",
  "license": "MIT",
  "devDependencies": {
    "cloc": "^2.5.1",
    "codecov": "^3.6.5",
    "esm": "^3.2.25",
    "flatqueue": "1.1.0",
    "mocha": "^7.0.1",
    "nyc": "^15.0.0",
    "tinyqueue": "2.0.3"
  },
  "nyc": {
    "all": true,
    "include": [
      "heapify.mjs"
    ]
  },
  "dependencies": {}
}
